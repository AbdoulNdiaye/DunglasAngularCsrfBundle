{
    "name": "dunglas/angular-csrf-bundle",
    "type": "symfony-bundle",
    "description": "CSRF protection when using AngularJS with Symfony2",
    "keywords": ["csrf", "xsrf", "security", "angularJS"],
    "homepage": "http://dunglas.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "KÃ©vin Dunglas",
            "email": "dunglas@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/dependency-injection": "~2.4",
        "symfony/config": "~2.4",
        "symfony/http-foundation": "~2.4",
        "symfony/http-kernel": "~2.4",
        "symfony/security-csrf": "~2.4"
    },
    "require-dev": {
        "phpspec/phpspec": "2.0.*@dev"
    },
    "autoload": {
        "psr-0": { "Dunglas\\AngularCsrfBundle": "" }
    },
    "target-dir": "Dunglas/AngularCsrfBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}